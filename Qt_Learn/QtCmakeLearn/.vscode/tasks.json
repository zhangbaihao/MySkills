{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "Build",
		"type": "shell",
		"command": "cmake --build ${workspaceFolder}/build --config ${command:cmake.buildType}",
		"group": {
		  "kind": "build",
		  "isDefault": true
		}
	  },
	  {
		"label": "Clean",
		"type": "shell",
		"command": "cmake --build ${workspaceFolder}/build --target clean",
		"group": {
		  "kind": "build",
		  "isDefault": true
		}
	  }
	]
  }